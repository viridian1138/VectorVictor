<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SymbolDeleteDialog" >

    <Button
        android:id="@+id/createVarButton"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_x="544dp"
        android:layout_y="404dp"
        android:text="Create..." />

    <Button
        android:id="@+id/createVarKeyboardButton"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_x="393dp"
        android:layout_y="405dp"
        android:text="Keyboard..." />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="18dp"
        android:layout_y="37dp"
        android:text="Var Name :"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <verdantium.mathimage.MathImageTextEdit
        android:id="@+id/createVarEditField"
        android:layout_width="944dp"
        android:layout_height="213dp"
        android:layout_x="22dp"
        android:layout_y="72dp"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Spinner
        android:id="@+id/createScriptSpinner"
        android:layout_width="164dp"
        android:layout_height="62dp"
        android:layout_x="18dp"
        android:layout_y="320dp" />

    <Button
        android:id="@+id/createInsertSymbolButton"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_x="215dp"
        android:layout_y="326dp"
        android:text="Insert..." />

    <Spinner
        android:id="@+id/createVarTypeSpinner"
        android:layout_width="163dp"
        android:layout_height="60dp"
        android:layout_x="26dp"
        android:layout_y="406dp" />

</AbsoluteLayout>
